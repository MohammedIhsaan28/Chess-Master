    "defaults",
    "not ie 11",
    "not edge <= 133",
    "not and_chr <= 133",
    "not chrome <= 133",
    "not firefox <= 135",
    "not safari <= 18.3",
    "not samsung <= 27",
    "not ios_saf <= 18.3"
  ]
}